FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
front
     { 
           type wedge;
      }
back
     { 
           type wedge;
      }

     tankWall
     { 
           type            fixedFluxPressure;
      }
     bottom
     { 
           type            fixedFluxPressure;
      }
     top
     { 
           type            fixedFluxPressure;
      }
    defaultFaces
    {
        type            empty;
    }
}


