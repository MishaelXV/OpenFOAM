FoamFile
{
    version         2.0;
    format          ascii;
 
    root            "";
    case            "";
    instance        "";
    local           "";
 
    class           dictionary;
    object          blockMeshDict;
}
 
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
 
convertToMeters 0.01;
 
vertices
(
(0 0 0)
(1 0.05 0)
(1 0.05 1) 
(0 0 1)
(1 -0.05 0) 
(1 -0.05 1)
);
 
blocks
(
hex (0 4 1 0 3 5 2 3) (250  1 250) simpleGrading (1 1 1)
);
 
